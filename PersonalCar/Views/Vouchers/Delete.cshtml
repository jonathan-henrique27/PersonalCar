                            @Html.DisplayNameFor(model => model.UnidadeDeNegocio.NomeFantasia)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.UnidadeDeNegocio.NomeFantasia)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.DataSolicitacao)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.DataSolicitacao)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.DataInicial)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.DataInicial)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.DataFinal)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.DataFinal)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.ValorPedagio)
