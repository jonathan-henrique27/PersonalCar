                            @Html.DisplayFor(model => model.ValorTotalDaViagem)
                        </dd>
                    </dl>
                </div>
                <div class="modal-footer">
                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()
                        <div class="form-actions no-color">
                            <input id="botao" type="submit" value="Deletar" class="btn btn-danger" />
                            <button id="botao" type="button" class="btn btn-success" data-dismiss="modal">Cancelar</button>
                        </div>
                    }
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
        
    </div>